'Dias habiles
Set rng = Worksheets("Hoja1").Range("B152")
Worksheets("PROGRAMACION CALIENTE 3").Range("G16").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value


End Sub

