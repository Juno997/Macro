'NUMERO DE ALUMNOS, AJUSTES, DIAS HABILES,
'Costalitos
Dim rng As Range
Set rng = Worksheets("Hoja1").Range("B4")
Worksheets("PROGRAMACION CALIENTE").Range("H14").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Aceite
Set rng = Worksheets("Hoja1").Range("D4")
Worksheets("PROGRAMACION CALIENTE").Range("I14").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

