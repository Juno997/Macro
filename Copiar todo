
Sub CopiarTodo()
Call CopiarDatos
Call CopiarDatos2
Call CopiarDatos3
Call CopiarDatos4
Call CopiarDatos5
Call CopiarProgramacion
Call CopiarProgramacion2
Call CopiarProgramacion3
Call CopiarProgramacion4
Call CopiarProgramacion5
Call CopiarProgramacion6
Call CopiarProgramacion7
Call CopiarProgramacion8
Call CopiarProgramacion9
End Sub


Sub CopiarDatos()
'Primera Escuela

'Costalitos
Dim rng As Range
Set rng = Worksheets("Hoja1").Range("B4")
Worksheets("Desayuno Caliente").Range("C16").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Aceite
Set rng = Worksheets("Hoja1").Range("D4")
Worksheets("Desayuno Caliente").Range("C17").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Harina
Set rng = Worksheets("Hoja1").Range("E4")
Worksheets("Desayuno Caliente").Range("C18").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Litros de Leche
Set rng = Worksheets("Hoja1").Range("E2")
Worksheets("Desayuno Caliente").Range("C19").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Cajas litros
Set rng = Worksheets("Hoja1").Range("A4")
Worksheets("Desayuno Caliente").Range("D20").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Litros Sueltos
Set rng = Worksheets("Hoja1").Range("C4")
Worksheets("Desayuno Caliente").Range("D21").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value

'Total a Pagar (Cuota de recuperacion)
Set rng = Worksheets("Hoja1").Range("F2")
Worksheets("Desayuno Caliente").Range("G20").Resize(rng.Rows.Count, rng.Columns.Count).Cells.Value = rng.Cells.Value



