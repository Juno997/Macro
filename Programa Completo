    Sub PruebasDif11()
    

   '179TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 179", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D891") = "Ajustes"
Range("D892") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A891") = "Numero de Alumnos"
Range("A892") = Alumnos
Range("B891") = "Dias"
Range("B892") = Dias
Range("C891") = "Total"
Range("C892") = Total
Range("E891") = "Total de litros de leche"
Range("E892") = Totallitrosdeleche
Range("A893") = "Total de cajas"
Range("A894") = Cajasredondeadas
Range("B893") = "Costalitos"
Range("B894") = Costalitos
Range("C893") = "Litros sueltos"
Range("C894") = LitrosSueltos2
Range("D893") = "Aceite"
Range("D894") = Ajustesredondeado
Range("E893") = "Harina"
Range("E894") = Harina
Range("F891") = "Cuota de recuperacion"
Range("F892") = CuotaRecu
Range("F893") = "Numero de escuela"
Range("F894") = EscNum + 178



