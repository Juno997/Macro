Sub PruebasDif3()


'37th ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 37", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D181") = "Ajustes"
Range("D182") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A181") = "Numero de Alumnos"
Range("A182") = Alumnos
Range("B181") = "Dias"
Range("B182") = Dias
Range("C181") = "Total"
Range("C182") = Total
Range("A181:E184").Select
Range("E181") = "Total de litros de leche"
Range("E182") = Totallitrosdeleche
Range("A183") = "Total de cajas"
Range("A184 ") = Cajasredondeadas
Range("B183") = "Costalitos"
Range("B184") = Costalitos
Range("C183") = "Litros sueltos"
Range("C184") = LitrosSueltos2
Range("D183") = "Aceite"
Range("D184") = Ajustesredondeado
Range("E183") = "Harina"
Range("E184") = Harina
Range("F181") = "Cuota de recuperacion"
Range("F182") = CuotaRecu
Range("F183") = "Numero de escuela"
Range("F184") = EscNum + 36



