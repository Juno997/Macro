Range("E593") = "Harina"
Range("E594") = Harina
Range("F591") = "Cuota de recuperacion"
Range("F592") = CuotaRecu
Range("F593") = "Numero de escuela"
Range("F594") = EscNum + 118

'Formato de la tabla


    Range("A591:F591").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A593:F593").Select
    With Selection.Interior
        .Pattern = xlSolid
