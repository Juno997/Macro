        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.399975585192419
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A617:F617").Select
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A616:F619").Select
    Selection.Borders(xlDiagonalDown).LineStyle = xlNone
    Selection.Borders(xlDiagonalUp).LineStyle = xlNone
    With Selection.Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideVertical)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
End Sub

Sub PruebasDif8()

'125TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 125", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D621") = "Ajustes"
Range("D622") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3
'posiciones en la hoja

Range("A621") = "Numero de Alumnos"
Range("A622") = Alumnos
Range("B621") = "Dias"
Range("B622") = Dias
Range("C621") = "Total"
Range("C622") = Total
Range("E621") = "Total de litros de leche"
Range("E622") = Totallitrosdeleche
Range("A623") = "Total de cajas"
Range("A624 ") = Cajasredondeadas
Range("B623") = "Costalitos"
Range("B624") = Costalitos
Range("C623") = "Litros sueltos"
Range("C624") = LitrosSueltos2
Range("D623") = "Aceite"
Range("D624") = Ajustesredondeado
Range("E623") = "Harina"
Range("E624") = Harina
Range("F621") = "Cuota de recuperacion"
Range("F622") = CuotaRecu
Range("F623") = "Numero de escuela"
Range("F624") = EscNum + 124

'Formato de la tabla


    Range("A621:F621").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A623:F623").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A622:F622").Select
