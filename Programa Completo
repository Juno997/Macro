Sub PruebasDif10()
                
        
          '161TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 161", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D801") = "Ajustes"
Range("D802") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A801") = "Numero de Alumnos"
Range("A802") = Alumnos
Range("B801") = "Dias"
Range("B802") = Dias
Range("C801") = "Total"
Range("C802") = Total
Range("E801") = "Total de litros de leche"
Range("E802") = Totallitrosdeleche
Range("A803") = "Total de cajas"
Range("A804") = Cajasredondeadas
Range("B803") = "Costalitos"
Range("B804") = Costalitos
Range("C803") = "Litros sueltos"
Range("C804") = LitrosSueltos2
Range("D803") = "Aceite"
Range("D804") = Ajustesredondeado
Range("E803") = "Harina"
Range("E804") = Harina
Range("F801") = "Cuota de recuperacion"
Range("F802") = CuotaRecu
Range("F803") = "Numero de escuela"
Range("F804") = EscNum + 160


'Formato de la tabla


