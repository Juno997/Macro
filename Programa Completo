Sub PruebasDif2()

'19TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 19", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D91") = "Ajustes"
Range("D92") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3

Range("A91") = "Numero de Alumnos"
Range("A92") = Alumnos
Range("B91") = "Dias"
Range("B92") = Dias
Range("C91") = "Total"
Range("C92") = Total
Range("A91:E92").Select
Range("E91") = "Total de litros de leche"
Range("E92") = Totallitrosdeleche
Range("A93") = "Total de cajas"
Range("A94") = Cajasredondeadas
Range("B93") = "Costalitos"
Range("B94") = Costalitos
Range("C93") = "Litros sueltos"
Range("C94") = LitrosSueltos2
Range("D93") = "Aceite"
Range("D94") = Ajustesredondeado
Range("E93") = "Harina"
Range("E94") = Harina
Range("F91") = "Cuota de recuperacion"
Range("F92") = CuotaRecu
Range("F93") = "Numero de escuela"
Range("F94") = EscNum + 18


'Formato de la tabla


    Range("A91:F91").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
