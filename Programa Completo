'200TH ESCUELA

' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 200", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D996") = "Ajustes"
Range("D997") = Ajustes
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)

Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A996") = "Numero de Alumnos"
Range("A997") = Alumnos
Range("B996") = "Dias"
Range("B997") = Dias
Range("C996") = "Total"
Range("C997") = Total
Range("E996") = "Total de litros de leche"
Range("E997") = Totallitrosdeleche
Range("A998") = "Total de cajas"
Range("A999") = Cajasredondeadas
Range("B998") = "Costalitos"
Range("B999") = Costalitos
Range("C998") = "Litros sueltos"
Range("C999") = LitrosSueltos2
Range("D998") = "Aceite"
Range("D999") = Ajustesredondeado
Range("E998") = "Harina"
Range("E999") = Harina
Range("F996") = "Cuota de recuperacion"
Range("F997") = CuotaRecu
Range("F998") = "Numero de escuela"
Range("F999") = EscNum + 199

