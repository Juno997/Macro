Sub PruebasDif5()


   '73TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 73", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D361") = "Ajustes"
Range("D362") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3
'posiciones en la hoja

Range("A361") = "Numero de Alumnos"
Range("A362") = Alumnos
Range("B361") = "Dias"
Range("B362") = Dias
Range("C361") = "Total"
Range("C362") = Total
Range("E361") = "Total de litros de leche"
Range("E362") = Totallitrosdeleche
Range("A363") = "Total de cajas"
Range("A364 ") = Cajasredondeadas
Range("B363") = "Costalitos"
Range("B364") = Costalitos
Range("C363") = "Litros sueltos"
Range("C364") = LitrosSueltos2
Range("D363") = "Aceite"
Range("D364") = Ajustesredondeado
Range("E363") = "Harina"
Range("E364") = Harina
Range("F361") = "Cuota de recuperacion"
Range("F362") = CuotaRecu
Range("F363") = "Numero de escuela"
Range("F364") = EscNum + 72

'Formato de la tabla


    Range("A361:F361").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
