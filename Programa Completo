'184TH ESCUELA

' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 184", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D916") = "Ajustes"
Range("D917") = Ajustes
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)

Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A916") = "Numero de Alumnos"
Range("A917") = Alumnos
Range("B916") = "Dias"
Range("B917") = Dias
Range("C916") = "Total"
Range("C917") = Total
Range("E916") = "Total de litros de leche"
Range("E917") = Totallitrosdeleche
Range("A918") = "Total de cajas"
Range("A919") = Cajasredondeadas
Range("B918") = "Costalitos"
Range("B919") = Costalitos
Range("C918") = "Litros sueltos"
Range("C919") = LitrosSueltos2
Range("D918") = "Aceite"
Range("D919") = Ajustesredondeado
Range("E918") = "Harina"
Range("E919") = Harina
Range("F916") = "Cuota de recuperacion"
Range("F917") = CuotaRecu
Range("F918") = "Numero de escuela"
Range("F919") = EscNum + 183

