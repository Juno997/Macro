
Sub ProgramaCompleto()
Call PruebasDif
Call PruebasDif2
Call PruebasDif3
Call PruebasDif4
Call PruebasDif5
Call PruebasDif6
Call PruebasDif7
Call PruebasDif8
Call PruebasDif9
Call PruebasDif10
Call PruebasDif11
Call PruebasDif12
Call totales
End Sub

Sub PruebasDif()

'PRIMER ESCUELA

'Variables

Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = InputBox("Teclee los dias habiles del mes ", "Create Invoice Number", 1)
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 1", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D1") = "Ajustes"
Range("D2") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Range("D5") = Ajustesredondeado
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Integer
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos
Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3
Dim Aceite As Integer
Aceite = Ajustesredondeado



'posiciones en la hoja

Range("A1") = "Numero de Alumnos"
Range("A2") = Alumnos
Range("B1") = "Dias"
Range("B2") = Dias
Range("C1") = "Total"
Range("C2") = Total
Range("A1:E5").Select
Range("E1") = "Total de litros de leche "
Range("E2") = Totallitrosdeleche
Range("A3") = "Total de cajas"
Range("A4 ") = Cajasredondeadas
Range("B3") = "Costalitos"
Range("B4") = Costalitos
Range("C3") = "Litros sueltos"
Range("C4") = LitrosSueltos2
Range("D3") = "Aceite"
Range("D4") = Aceite
Range("E3") = "Harina"
Range("E4") = Harina
Range("F1") = "Cuota de recuperacion"
Range("F2") = CuotaRecu
Range("F3") = "Numero de escuela"
Range("F4") = EscNum



'Formato de la tabla


    Range("A1:F1").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A3:F3").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A2:F2").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.399975585192419
        .PatternTintAndShade = 0
    End With
    Range("A4:F4").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.399975585192419
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A2:F2").Select
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A1:F4").Select
    Selection.Borders(xlDiagonalDown).LineStyle = xlNone
    Selection.Borders(xlDiagonalUp).LineStyle = xlNone
    With Selection.Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideVertical)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    
'2DA ESCUELA

' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 2", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D6") = "Ajustes"
Range("D7") = Ajustes
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)

Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos
CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

'Posicion en la hoja

Range("A6") = "Numero de Alumnos"
Range("A7") = Alumnos
Range("B6") = "Dias"
Range("B7") = Dias
Range("C6") = "Total"
Range("C7") = Total
Range("A6:E7").Select
Range("E6") = "Total de litros de leche"
Range("E7") = Totallitrosdeleche
Range("A8") = "Total de cajas"
Range("A9 ") = Cajasredondeadas
Range("B8") = "Costalitos"
Range("B9") = Costalitos
Range("C8") = "Litros sueltos"
Range("C9") = LitrosSueltos2
Range("D8") = "Aceite"
Range("D9") = Ajustesredondeado
Range("E8") = "Harina"
Range("E9") = Harina
Range("F6") = "Cuota de recuperacion"
Range("F7") = CuotaRecu
Range("F8") = "Numero de escuela"
Range("F9") = EscNum + 1


'Formato de la tabla


    Range("A6:F6").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A8:F8").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    Range("A7:F7").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.399975585192419
        .PatternTintAndShade = 0
    End With
    Range("A9:F9").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent1
        .TintAndShade = 0.399975585192419
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A7:F7").Select
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
    Range("A6:F9").Select
    Selection.Borders(xlDiagonalDown).LineStyle = xlNone
    Selection.Borders(xlDiagonalUp).LineStyle = xlNone
    With Selection.Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideVertical)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With Selection.Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    
    '3RD ESCUELA

' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 3", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D11") = "Ajustes"
Range("D12") = Ajustes
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos


CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

'Posicion en la hoja

Range("A11") = "Numero de Alumnos"
Range("A12") = Alumnos
Range("B11") = "Dias"
Range("B12") = Dias
Range("C11") = "Total"
Range("C12") = Total
Range("A11:E12").Select
Range("E11") = "Total de litros de leche"
Range("E12") = Totallitrosdeleche
Range("A13") = "Total de cajas"
Range("A14") = Cajasredondeadas
Range("B13") = "Costalitos"
Range("B14") = Costalitos
Range("C13") = "Litros sueltos"
Range("C14") = LitrosSueltos2
Range("D13") = "Aceite"
Range("D14") = Ajustesredondeado
Range("E13") = "Harina"
Range("E14") = Harina
Range("F11") = "Cuota de recuperacion"
Range("F12") = CuotaRecu
Range("F13") = "Numero de escuela"
Range("F14") = EscNum + 2


'Formato de la tabla


    Range("A11:F11").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
