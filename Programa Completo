'109TH ESCUELA
' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 109", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D541") = "Ajustes"
Range("D542") = Ajustes
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)

Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A541") = "Numero de Alumnos"
Range("A542") = Alumnos
Range("B541") = "Dias"
Range("B542") = Dias
Range("C541") = "Total"
Range("C542") = Total
Range("E541") = "Total de litros de leche"
Range("E542") = Totallitrosdeleche
Range("A543") = "Total de cajas"
Range("A544 ") = Cajasredondeadas
Range("B543") = "Costalitos"
Range("B544") = Costalitos
Range("C543") = "Litros sueltos"
Range("C544") = LitrosSueltos2
Range("D543") = "Aceite"
Range("D544") = Ajustesredondeado
Range("E543") = "Harina"
Range("E544") = Harina
Range("F541") = "Cuota de recuperacion"
Range("F542") = CuotaRecu
Range("F543") = "Numero de escuela"
Range("F544") = EscNum + 108

