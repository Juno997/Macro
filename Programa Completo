'23RD ESCUELA

' Operaciones

Alumnos = InputBox("Teclee el numero de alumnos de la escuela 23", "Create Invoice Number", 1)


Total = Dias * Alumnos

Ajustes = Total / 100
Range("D111") = "Ajustes"
Range("D112") = Ajustes

Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Totallitrosdeleche = Ajustesredondeado * 25

Cajas = Totallitrosdeleche / 12
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Costalitos = Ajustesredondeado

LitrosSueltos = Cajasredondeadas * 12
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

CuotaRecu = Ajustesredondeado * 50
Harina = Ajustesredondeado * 3

