    Sub PruebasDif12()
    

     '197TH ESCUELA
'Variables
Dim EscNum As Integer
EscNum = 1
Dim Dias As Integer
Dias = Range("B2")
Dim Alumnos As Integer
Alumnos = InputBox("Teclee el numero de alumnos de la escuela 197", "Create Invoice Number", 1)
Dim Total As Integer
Total = Dias * Alumnos
Dim Ajustes As Double
Ajustes = Total / 100
Range("D981") = "Ajustes"
Range("D982") = Ajustes
Dim Ajustesredondeado As Integer
Ajustesredondeado = Application.WorksheetFunction.RoundDown(Ajustes, 0)
Dim Totallitrosdeleche As Integer
Totallitrosdeleche = Ajustesredondeado * 25
Dim Cajas As Double
Cajas = Totallitrosdeleche / 12
Dim Cajasredondeadas As Integer
Cajasredondeadas = Application.WorksheetFunction.RoundDown(Cajas, 0)
Dim Costalitos As Double
Costalitos = Ajustesredondeado
Dim LitrosSueltos As Double
LitrosSueltos = Cajasredondeadas * 12
Dim LitrosSueltos2 As Integer
LitrosSueltos2 = Totallitrosdeleche - LitrosSueltos

Dim CuotaRecu As Integer
CuotaRecu = Ajustesredondeado * 50
Dim Harina As Integer
Harina = Ajustesredondeado * 3

'posiciones en la hoja

Range("A981") = "Numero de Alumnos"
Range("A982") = Alumnos
Range("B981") = "Dias"
Range("B982") = Dias
Range("C981") = "Total"
Range("C982") = Total
Range("E981") = "Total de litros de leche"
Range("E982") = Totallitrosdeleche
Range("A983") = "Total de cajas"
Range("A984") = Cajasredondeadas
Range("B983") = "Costalitos"
Range("B984") = Costalitos
Range("C983") = "Litros sueltos"
Range("C984") = LitrosSueltos2
Range("D983") = "Aceite"
Range("D984") = Ajustesredondeado
Range("E983") = "Harina"
Range("E984") = Harina
Range("F981") = "Cuota de recuperacion"
Range("F982") = CuotaRecu
Range("F983") = "Numero de escuela"
Range("F984") = EscNum + 196



'Formato de la tabla


    Range("A981:F981").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
