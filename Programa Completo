Range("F887") = CuotaRecu
Range("F888") = "Numero de escuela"
Range("F889") = EscNum + 177

'Formato de la tabla


    Range("A886:F886").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
