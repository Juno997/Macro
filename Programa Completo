Range("F727") = CuotaRecu
Range("F728") = "Numero de escuela"
Range("F729") = EscNum + 145

'Formato de la tabla


    Range("A726:F726").Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -0.749992370372631
        .PatternTintAndShade = 0
    End With
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
